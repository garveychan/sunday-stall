<li>
  <div class="space-y-4">
    <a href=<%= stall_path(id: stall.id) %>>
      <div class="aspect-w-3 aspect-h-2 card">
        <% if stall.image.attached? %>
          <img class="w-96 h-64 object-cover shadow-lg rounded-lg" src= "<%= asset_path(url_for(stall.image)) %>" alt= "<%= stall.title %>">
        <% end %>
      </div>
    </a>
    <div class="space-y-2">
      <div class="text-lg leading-6 font-medium space-y-1">
        <h3><%= stall.title %></h3>
        <p class="text-indigo-600"><%= stall.subtitle %></p>
      </div>
    </div>
  </div>
</li>
